<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane
        maxHeight="-Infinity"
        maxWidth="-Infinity"
        minHeight="-Infinity"
        minWidth="-Infinity"
        prefHeight="400.0"
        prefWidth="600.0"
        xmlns="http://javafx.com/javafx/8.0.112"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="Client.Room.RoomViewController"
>
    <children>
        <TextArea fx:id="chatLog" editable="false" layoutX="4.0" layoutY="4.0" prefHeight="341.0" prefWidth="590.0" />
        <TextField fx:id="myMessage" layoutX="4.0" layoutY="349.0" onKeyPressed="#buttonPressed" prefHeight="47.0" prefWidth="536.0" />
        <Button
                layoutX="543.0"
                layoutY="349.0"
                mnemonicParsing="false"
                onAction="#onClickSend"
                onKeyPressed="#buttonPressed"
                prefHeight="47.0" prefWidth="52.0"
                text="Enviar" />
    </children>
</Pane>
